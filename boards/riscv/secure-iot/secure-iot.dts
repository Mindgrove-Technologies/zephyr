/dts-v1/;

#include <secure-iot/riscv64-secure-iot.dtsi>

/ {
	model = "shakti,spike-bare";
	compatible = "shakti,spike-bare-dev";

chosen {
		zephyr,console = &uart0;
	};
};


&uart0 {
	status = "okay";
	current-speed = <115200>;
	clock-frequency = <100000000>;
};

&spi0 {
	status = "okay";
	db_configure = <0x1 0x0 4>;
}